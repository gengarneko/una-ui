    "@una/tsconfig": "workspace:^",
    "@una/vite": "workspace:^",
    "autoprefixer": "^10.4.20",
    "globby": "^14.0.2",
    "playwright": "^1.46.1",
    "postcss": "^8.4.44",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.1.0",
    "vite-plugin-external": "^4.3.1"
  }
